<script type="text/javascript">
      $(document).ready(function () {
        HentMelding();
    });
    function HentMelding() {
        $.ajax({
            url: '/api/ProsjektreqApi',
            type: 'GET',
            dataType: 'json',
            success: function (data) {
                VisAlle(data);
            },
            error: function (x, y, z) {
                alert(x + '\n' + y + '\n' + z);
            }
        });
    }
    function VisAlle(melding) {
        
var strResult = "Dine meldinger";
        $.each(melding, function (i, m) {
            strResult += "<p> " + m.Tid + " " + m.FraBruker + " "+m.Melding +" "+ m.Prosjektnavn +" <button class='btn btn-primary'>Godta</button></p>";
        });
        $("#meldinger").html(strResult);
 }

